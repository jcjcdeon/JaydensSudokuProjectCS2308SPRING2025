#include <iostream>

void printBoard(int board[9][9]) {
    for (int i = 0; i < 9; ++i) {
        if (i % 3 == 0 && i != 0)
            std::cout << "------+-------+------\n";

        for (int j = 0; j < 9; ++j) {
            if (j % 3 == 0 && j != 0)
                std::cout << "| ";

            if (board[i][j] == 0)
                std::cout << ". ";
            else
                std::cout << board[i][j] << " ";
        }
        std::cout << "\n";
    }
}
